{
  "name": "eject-enum",
  "version": "0.1.0",
  "description": "",
  "main": "./dist/index.js",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.esm.js"
    }
  },
  "typings": "./dist/index.d.ts",
  "bin": "./bin/cli.js",
  "files": [
    "dist",
    "bin"
  ],
  "scripts": {
    "prepare": "husky install",
    "exec-ts": "node -r esbuild-register",
    "lint": "run-p tsc lint:*",
    "lint:format": "prettier --check --loglevel warn {src,test}/**/*.ts",
    "lint:ts": "eslint --max-warnings 0 src/ test/",
    "fix": "run-s fix:*",
    "fix:format": "prettier --write --loglevel warn {src,test}/**/*.ts",
    "fix:ts": "eslint --fix src/ test/",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "esbuild": "node build.js",
    "build": "run-s type-check esbuild",
    "buildw": "nodemon -e js,ts --ignore dist/ build.js"
  },
  "author": "jiftechnify",
  "license": "MIT",
  "dependencies": {
    "ts-morph": "^15.1.0",
    "yargs": "^17.5.1"
  },
  "devDependencies": {
    "@tsconfig/strictest": "^1.0.1",
    "@types/fs-extra": "^9.0.13",
    "@types/node": "^17.0.35",
    "@types/yargs": "^17.0.10",
    "@typescript-eslint/eslint-plugin": "^5.30.0",
    "@typescript-eslint/parser": "^5.30.0",
    "esbuild": "^0.14.39",
    "esbuild-register": "^3.3.2",
    "eslint": "^8.18.0",
    "eslint-config-prettier": "^8.5.0",
    "fs-extra": "^10.1.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "nodemon": "^2.0.18",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "typescript": "^4.7.4",
    "vitest": "^0.16.0"
  },
  "lint-staged": {
    "src/**/*.{js,ts}": [
      "eslint --max-warnings 0",
      "prettier --check --loglevel warn"
    ]
  }
}
